{% extends "./main.njk" %}

{% block content %}
{%  include "partials/page-header.njk" %}
<section class="text-slate-700 body-font">
    <div class="container px-5 mx-auto helpArticle">
        {{  content | safe }}
    </div>
</section>
<section>
    <div class="container mx-auto py-5 pb-20 px-6 md:px-4">
        <div class="text-center px-4 py-5 text-slate-900">
            <h2 class="sm:text-3xl text-2xl font-medium title-font mb-2 mx-auto">Still need help?</h2>
            <p class="leading-relaxed text-base">Let us know what questions, comments, or concerns you have!</p>
            <p class="leading-relaxed text-base my-4">
                <a class="mx-auto text-white btn btn-blue" href="https://github.com/MisterPhilip/omnibug/issues/new/choose" rel="nofollow">Send Feedback</a>
            </p>
        </div>
    </div>
</section>
{% endblock %}
