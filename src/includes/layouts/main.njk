<!doctype html>
<html lang="en">
<head>
    <link rel="stylesheet" href="/assets/css/main.css" />
    {% if title.includes("|") %}
        <title>{{  title  }}</title>
    {%  else %}
        <title>{{  title  }} | Omnibug</title>
    {%  endif %}
